# GitLab.com
Host gitlab.com
  PreferredAuthentications publickey
  AddKeysToAgent yes
  IdentityFile ~/.ssh/gitlab
  IdentitiesOnly=yes
  ServerAliveInterval 60
  ServerAliveCountMax 240
Host github.com
  PreferredAuthentications publickey
  AddKeysToAgent yes
  IdentityFile ~/.ssh/github
  IdentitiesOnly=yes
  ServerAliveInterval 60
  ServerAliveCountMax 240  
# Lambda server
Host lambda
  HostName 10.18.168.55
  User shravan
  IdentityFile /home/shravan/.ssh/dannerlab-lambda