     HostName 172.21.0.2
     User tatarama
     IdentityFile /Users/tatarama/.ssh/synology-nas
     # IdentitiesOnly=yes
     # ServerAliveInterval 60
     # ServerAliveCountMax 240
     ProxyJump vpsserver
