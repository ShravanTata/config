# NAS
Host vpsserver
     HostName tatarama.my.to
     User debian
     IdentityFile /Users/tatarama/.ssh/vpsserver
     # Port 2536
     IdentitiesOnly=yes
     ServerAliveInterval 60
     ServerAliveCountMax 240
Host synology-nas
     HostName 172.21.0.2
     User tatarama
     IdentityFile /Users/tatarama/.ssh/synology-nas
     # IdentitiesOnly=yes
     # ServerAliveInterval 60
     # ServerAliveCountMax 240
     ProxyJump vpsserver
Host synology-nas-local
     HostName 10.18.148.66
     User tatarama
     IdentityFile /Users/tatarama/.ssh/synology-nas
     # IdentitiesOnly=yes
     # ServerAliveInterval 60
     # ServerAliveCountMax 240
     # ProxyJump vpsserver
